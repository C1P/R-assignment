12模拟计算定积分
A<- function(n){
    k <- 0
    x <- runif(n)
    y <- runif(n) * sqrt(2)
    for(i in 1:n){
        if (y[i] <= sqrt(1 + x[i]^2))
            k <- k + 1
    }
    k * sqrt(2) / n
}
A(100000)
## [1] 1.146461
